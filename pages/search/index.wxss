page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
  background-color: #fff;
  border-radius: 10rpx;
}
.search_row .search_input {
  flex: 1;
  height: 100%;
  padding-left: 30rpx;
}
.search_row .search_cancel {
  width: 110rpx;
  height: 100%;
  font-size: 28rpx;
  padding: 0;
  text-align: center;
  line-height: 60rpx;
  margin-left: 10rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.search_content {
  margin-top: 10rpx;
}
.search_content .search_item {
  background-color: #fff;
  border: 1rpx solid #dedede;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border-radius: 10rpx;
}
