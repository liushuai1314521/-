.btm_tool {
  padding: 20rpx 0;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 125rpx;
  background-color: #fff;
  display: flex;
  text-align: center;
}
.btm_tool .tool_item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}
.btm_tool .tool_item .car_total {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  color: white;
  border: 2rpx solid currentColor;
  border-radius: 50%;
  background-color: var(--themeColor);
  margin-top: -40%;
  margin-left: 20%;
  font-size: 18rpx;
  padding: 6rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btm_tool .tool_item .btn_false {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.btm_tool .tool_item .car_txt {
  font-size: 24rpx;
  color: #999999;
}
.btm_tool .btn_cart {
  flex: 2;
  background-color: #FF9500;
  color: white;
  font-weight: 600;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
  margin-left: 5rpx;
}
.btm_tool .btn_buy {
  flex: 2;
  color: white;
  background-color: #FF0036;
  font-weight: 600;
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  margin-right: 5rpx;
}
.bottom_youhua {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 48rpx;
  background-color: #fff;
}
page {
  padding-bottom: 120rpx;
}
