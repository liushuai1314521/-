.price_star {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.price_star .iconfont {
  margin-right: 5%;
  font-size: 40rpx;
}
.price_star .icon_shoucang1 {
  background-color: #FF0036;
  border: 2rpx solid black;
}
.price_star .goods_price {
  padding: 5rpx 25rpx;
  font-size: 35rpx;
  font-weight: 600;
  color: var(--themeColor);
  border-top: 1px solid #F9F9F9;
}
.goods_name_row {
  border-bottom: 4rpx solid #F9F9F9;
  display: flex;
  padding: 10rpx 0;
}
.goods_name_row .goods_name {
  color: #333333;
  padding: 0 10rpx;
  font-size: 30rpx;
  font-weight: 600;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.goods_name_row .link_title {
  color: var(--themeColor);
  font-size: 25rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-decoration: underline;
  padding: 6rpx 10rpx;
}
.goods_name_row .tit {
  flex: 5;
}
.goods_name_row .goods_collect {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-left: 2rpx solid #F9F9F9;
